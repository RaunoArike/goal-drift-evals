-- Add indexes
CREATE INDEX idx_trade_id ON trades(trade_id);
CREATE INDEX idx_strategy_id ON strategies(strategy_id);
CREATE INDEX idx_manager_id ON portfolio_managers(manager_id);
CREATE INDEX idx_trade_date ON trades(trade_date);

